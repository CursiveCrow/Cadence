{
  "name": "@cadence/core",
  "version": "0.0.1",
  "private": true,
  "description": "Domain types, algorithms (DAG, lanes), validation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist *.tsbuildinfo",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest -c ../../jest.config.js"
  },
  "files": [
    "dist"
  ],
