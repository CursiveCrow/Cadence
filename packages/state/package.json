                     },
    "devDependencies":  {
                            "@types/react":  "^18.2.45",
                            "typescript":  "^5.3.3",
                            "eslint":  "^8.56.0",
                            "@typescript-eslint/eslint-plugin":  "^6.15.0",
                            "@typescript-eslint/parser":  "^6.15.0"
                        },
    "peerDependencies":  {
                             "react":  "^18.2.0",
                             "@cadence/core":  "workspace:*"
                         }
}
