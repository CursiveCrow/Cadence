{
    "name": "cadence",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "main": "dist-electron/main.cjs",
    "scripts": {
        "dev": "concurrently \"npm:dev:renderer\" \"npm:dev:tsup\" \"npm:dev:electron\"",
        "dev:renderer": "vite",
        "dev:tsup": "tsup electron/main.ts electron/preload.ts --format cjs --out-dir dist-electron --sourcemap --watch --onSuccess \"echo electron build updated\"",
        "dev:electron": "wait-on http://localhost:5173 file:dist-electron/main.cjs && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
        "build:renderer": "vite build",
        "build:electron": "tsup electron/main.ts electron/preload.ts --format cjs --out-dir dist-electron --sourcemap",
        "build": "npm run build:renderer && npm run build:electron",
        "start": "cross-env NODE_ENV=production electron ."
    },
    "devDependencies": {
        "@types/node": "^20.14.10",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "concurrently": "^9.0.1",
        "cross-env": "^7.0.3",
        "tsup": "^8.2.4",
        "vite": "^5.4.0",
        "wait-on": "^7.2.0"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.8.2",
        "electron": "^31.2.1",
        "pixi.js": "^8.12.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-redux": "^9.2.0"
    }
}